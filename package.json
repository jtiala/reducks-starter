{
  "name": "reducks-starter",
  "version": "0.1.0",
  "description": "Reducks Starter is a highly opinionated React/Redux boilerplate based on the re-ducks approach.",
  "author": "Joonas Tiala <joonas.tiala@gmail.com>",
  "keywords": [
    "starter",
    "boilerplate",
    "bootstrap",
    "javascript",
    "react",
    "redux",
    "ducks",
    "reducks",
    "react-router",
    "immutable"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtiala/reducks-starter"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --watch --watch-content-base --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "build:stats": "NODE_ENV=production webpack --mode production --profile --json > webpack-stats.json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer webpack-stats.json dist",
    "deploy": "./scripts/deploy-gh-pages.sh",
    "format": "prettier --write '**/*.{js,jsx,css,scss,html,md,json,yml,yaml}'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:other",
    "lint:fix": "npm run lint:js:fix && npm run lint:styles:fix",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "lint:js:fix": "eslint src/**/*.{js,jsx} --fix",
    "lint:styles": "stylelint src/**/*.{css,scss}",
    "lint:styles:fix": "stylelint src/**/*.{css,scss} --fix",
    "lint:other": "prettier --list-different '**/*.{md,json,yml,yaml}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "test:coverage": "jest --coverage",
    "test:report": "jest --reporters=default --reporters=jest-junit",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.15.0",
    "eslint-plugin-react-hooks": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "storybook-react-router": "^1.0.8",
    "stylelint": "^11.0.0",
    "stylelint-config-idiomatic-order": "^7.0.3",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-prettier": "^1.1.1",
    "terser-webpack-plugin": "^2.1.2",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
