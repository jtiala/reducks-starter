{
  "name": "reducks-starter",
  "version": "0.1.0",
  "description": "Reducks Starter",
  "author": "Joonas Tiala <joonas.tiala@gmail.com>",
  "keywords": [
    "starter",
    "boilerplate",
    "bootstrap",
    "javascript",
    "react",
    "redux",
    "ducks",
    "reducks",
    "react-router",
    "immutablejs"
  ],
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jtiala/reducks-starter"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "start": "NODE_ENV=development webpack-dev-server --watch --watch-content-base --mode development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "yarn lint:js && yarn lint:styles && lint:other",
    "lint:fix": "yarn lint:js:fix && yarn lint:styles:fix",
    "lint:js": "eslint src/**/*.{js,jsx}",
    "lint:js:fix": "eslint src/**/*.{js,jsx} --fix",
    "lint:styles": "stylelint src/**/*.{css,scss}",
    "lint:styles:fix": "stylelint src/**/*.{css,scss} --fix",
    "lint:other": "prettier --list-different '**/*.{md,json,yml,yaml}'",
    "format": "prettier --write '**/*.{js,jsx,css,scss,md,json,yml,yaml}'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "classnames": "^2.2.6",
    "fetch-mock": "^7.0.6",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.10",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.6.0",
    "stylelint-config-idiomatic-order": "^5.0.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-prettier": "^1.0.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
